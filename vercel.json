{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "functions": {
    "api/noc/register.js": {
      "maxDuration": 30 
    },
    "api/noc/all.js": {
      "maxDuration": 30 
    }
  }
}